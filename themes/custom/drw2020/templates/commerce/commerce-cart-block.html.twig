<div{{ attributes}} style="float:right">
  <div class="cart-block--summary">
    <a class="cart-block--link__expand" href="{{ url }}">
      
			<div id="iconocarritocompras">
			<div class="icontext">
				{# <div class="icon-wrap icon-xs bg-secondary round text-light"> #}


        <div class="icon-sm round border">


					<i class="fa fa-shopping-cart icono"></i>
					<span class="notify">{{ count }}</span>
				</div>
				<div class="text-wrap">
					<div></div>
				</div>
			</div> <!-- icontext.// -->
			</div>



    </a>
  </div>
  {% if content %}
  <div class="cart-block--contents">
    <div class="cart-block--contents__inner">
      <div class="cart-block--contents__items">
        {{ content }}
      </div>
      <div class="cart-block--contents__links">
        {{ links }}
      </div>
    </div>
  </div>
  {% endif %}
</div>










